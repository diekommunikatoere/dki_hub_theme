			"font": {
				"primary": "'Gibson', system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
				"secondary": "'Gill Sans', system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif"
			}
		},
		"blocks": {
			"core/image": {
				"appearanceTools": false,
				"dimensions": {
					"minHeight": true
				},
				"lightbox": {
					"allowEditing": true,
					"enabled": true
				}
			}
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"link": {
				"typography": { "fontWeight": "600", "textDecoration": "1px solid underline var(--wp--preset--color--primary)" },
				"color": { "text": "var:preset|color|primary" },
				":hover": {
					"typography": { "fontWeight": "600", "textDecoration": "1px solid underline var(--wp--preset--color--primary-hover)" },
					"color": { "text": "var:preset|color|primary-hover" }
				}
			},
			"h1": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"textTransform": "uppercase",
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"letterSpacing": "0.08em"
				},
				"color": { "text": "var:preset|color|primary" }
			},
			"h2": {
				"typography": { "fontFamily": "var(--wp--preset--font-family--heading)", "fontStyle": "normal", "fontWeight": "600", "letterSpacing": "0.03em" },
				"color": { "text": "var:preset|color|primary" }
			},
			"h3": {
				"typography": { "fontFamily": "var(--wp--preset--font-family--heading)", "fontStyle": "normal", "fontWeight": "600", "textTransform": "uppercase", "letterSpacing": "0.07em" },
				"color": { "text": "var:preset|color|tertiary-dark" }
			},
			"h4": {
				"typography": { "fontFamily": "var(--wp--preset--font-family--heading)", "fontStyle": "normal", "fontWeight": "600", "fontSize": "var(--wp--preset--font-size--large)" },
				"color": { "text": "var:preset|color|tertiary" }
			},
			"h5": {
				"typography": { "fontFamily": "var(--wp--preset--font-family--heading)", "fontSize": "var(--wp--preset--font-size--medium)", "fontStyle": "normal", "fontWeight": "600", "textTransform": "uppercase", "letterSpacing": "0.07em" },
				"color": { "text": "var:preset|color|tertiary-dark" }
			},
			"h6": {
				"typography": { "fontFamily": "var(--wp--preset--font-family--heading)", "fontStyle": "normal", "fontWeight": "600" },
				"color": { "text": "var:preset|color|tertiary-dark" }
			},
			"heading": {
				"typography": { "letterSpacing": "0.05em" },
				"color": { "text": "var:preset|color|primary" }
			},
			"caption": {
				"color": { "text": "var:preset|color|secondary" }
			},
			"button": {
				"color": { "text": "var:preset|color|contrast", "background": "var:preset|color|secondary" },
				"typography": { "fontSize": "var:preset|font-size|medium", "fontFamily": "var:preset|font-family|heading", "fontStyle": "normal", "fontWeight": "500" },
				"shadow": "var:preset|shadow|tiny",
				":hover": {
					"color": { "text": "var:preset|color|contrast", "background": "var:preset|color|secondary-hover" },
					"typography": { "fontSize": "var:preset|font-size|medium", "fontFamily": "var:preset|font-family|heading", "fontStyle": "normal", "fontWeight": "500", "textDecoration": "none" },
					"shadow": "var:preset|shadow|extra-small"
				},
				":active": {
					"color": { "background": "var:preset|color|secondary-hover" },
					"typography": { "fontSize": "var:preset|font-size|medium", "fontFamily": "var:preset|font-family|heading", "fontStyle": "normal", "fontWeight": "500" },
					"shadow": "var:preset|shadow|tiny"
				},
				"css": "{transition: all 0.2s}"
			}
		},
		"color": { "text": "#282616" },
		"spacing": { "blockGap": "1rem" },
		"blocks": {
			"core/quote": {
				"variations": {
					"plain": {
						"typography": { "fontSize": "var:preset|font-size|medium" }
					}
				}
			}
		}
	},
